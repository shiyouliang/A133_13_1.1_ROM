<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:orientation="vertical">    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content">        <TextView            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:layout_margin="2dp"            android:layout_weight="1"            android:gravity="center"            android:textSize="14sp"            android:text="@string/ddr_test_item_mem_size"            android:textColor="#FFFFFFFF" />        <TextView            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:layout_margin="2dp"            android:layout_weight="1"            android:textSize="14sp"            android:gravity="center"            android:text="@string/ddr_test_testing_progress"            android:textColor="#FFFFFFFF" />        <TextView            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:layout_margin="2dp"            android:layout_weight="1"            android:textSize="14sp"            android:gravity="center"            android:paddingHorizontal="20dp"            android:text="@string/ddr_test_testing_status"            android:textColor="#FFFFFFFF" />        <TextView            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:layout_margin="2dp"            android:layout_weight="1"            android:textSize="14sp"            android:gravity="center"            android:text="@string/ddr_test_error_count"            android:textColor="#FFFFFFFF" />        <TextView            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:layout_margin="2dp"            android:layout_weight="1"            android:textSize="14sp"            android:gravity="center"            android:text="错误数据"            android:textColor="#FFFFFFFF" />    </LinearLayout>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content">        <TextView            android:id="@+id/fileSize"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:layout_margin="2dp"            android:layout_weight="1"            android:textColor="#FFFFFFFF"            android:textSize="14sp"            android:gravity="center" />        <TextView            android:id="@+id/cycles"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:layout_margin="2dp"            android:layout_weight="1"            android:textSize="14sp"            android:textColor="#FFFFFFFF"            android:gravity="center" />        <TextView            android:id="@+id/state"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:layout_margin="2dp"            android:layout_weight="1"            android:textSize="14sp"            android:textColor="#FFFFFFFF"            android:gravity="center" />        <TextView            android:id="@+id/errorCount"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:layout_margin="2dp"            android:layout_weight="1"            android:textSize="14sp"            android:textColor="#FF0000"            android:gravity="center" />        <TextView            android:id="@+id/errorData"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_gravity="center"            android:layout_margin="2dp"            android:textSize="14sp"            android:layout_weight="1"            android:textColor="#FF0000"            android:gravity="center" />    </LinearLayout>    <ImageView        android:layout_width="match_parent"        android:layout_height="1dp"        android:layout_marginTop="2dp"        android:background="#FFFFFFFF" /></LinearLayout>